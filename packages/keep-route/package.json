{
  "name": "@sv-kit/keep-route",
  "version": "1.0.0",
  "description": "Route keep-alive for SvelteKit: per-route state snapshots, scroll restoration, include/exclude and LRU.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "license": "MIT",
  "keywords": [
    "svelte",
    "sveltekit",
    "keep-alive",
    "route cache",
    "scroll restoration"
  ],
  "scripts": {
    "clean": "node -e \"try{require('fs').rmSync('dist',{recursive:true,force:true})}catch{}\"",
    "build": "tsc -p tsconfig.json",
    "test": "echo no-tests"
  },
  "peerDependencies": {
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "svelte": "^5.0.0",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
